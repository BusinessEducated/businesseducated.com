  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "dbuild": "docker build ./ -t businesseducated-com",
    "ddev": "docker run -p 8000:8000 businesseducated-com",
    "debug": "set DEBUG=* gatsby develop --host 0.0.0.0",
    "ndevelop": "netlify env:import .env.development && netlify dev",
    "nbuild": "netlify env:import .env.development && netlify dev",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "cross-env NODE_ENV=production gatsby build",
    "develop": "gatsby develop",
    "dev": "cross-env NODE_ENV=development npm run develop",
    "start": "cross-env NODE_ENV=production npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
